<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	
	<packaging>pom</packaging>
	
	<artifactId>mrprez</artifactId>
	<groupId>com.mrprez</groupId>
	<version>1.2</version>
	
	<scm>
		<connection>scm:git:git://github.com/mrprez/mrprez.git</connection>
		<developerConnection>scm:git:https://github.com/mrprez/mrprez.git</developerConnection>
	  <tag>mrprez-1.2</tag>
  </scm>
	
	<repositories>
		<repository>
			<id>MrPrezDeployRepo</id>
			<url>http://localhost:8080/nexus/content/repositories/releases/</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>MrPrezDeploySnapshot</id>
			<url>http://localhost:8080/nexus/content/repositories/snapshots/</url>
		</repository>
		<repository>
			<id>MrPrezThirdParty</id>
			<url>http://localhost:8080/nexus/content/repositories/thirdparty/</url>
		</repository>
	</repositories>
	<pluginRepositories>
		<pluginRepository>
			<id>MrPrezPluginRepo</id>
			<url>http://localhost:8080/nexus/content/repositories/releases/</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</pluginRepository>
	</pluginRepositories>
	<distributionManagement>
		<repository>
			<id>MrPrezDeployRepo</id>
			<url>http://localhost:8080/nexus/content/repositories/releases/</url>
		</repository>
		<snapshotRepository>
			<id>MrPrezDeploySnapshot</id>
			<url>http://localhost:8080/nexus/content/repositories/snapshots/</url>
		</snapshotRepository>
	</distributionManagement>
	
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.5.1</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
		</plugins>
	
	</build>
	
	
</project>